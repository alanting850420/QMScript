//後台 指定矩形區域內尋找特定顏色
Sub BG_FindColor(X1, Y1, X2, Y2, FColor)
  Dim XY, MyArray
  XY = Plugin.Bkgnd.FindColor(FlHwnd,X1, Y1, X2, Y2, FColor)
  MyArray = Split(XY, "|")
  retX = CInt(MyArray(0)) : retY = CInt(MyArray(1))
End Sub

//後台 指定矩形區域內模糊找色
Sub BG_FindColorEx(X1, Y1, X2, Y2, FColor, Mode, Percent)
  Dim XY, MyArray
  XY = Plugin.Bkgnd.FindColorEx(FlHwnd, X1, Y1, X2, Y2, FColor, Mode, Percent)
  MyArray = Split(XY, "|")
  retX = CInt(MyArray(0)) : retY = CInt(MyArray(1))
End Sub

//後台取得前景滑鼠座標
Sub BG_GetCursorPos()
  Dim MouseX, MouseY, sRect, MyArray
  GetCursorPos MouseX, MouseY
  sRect = Plugin.Window.GetWindowRect(FlHwnd)
  MyArray = Split(sRect, "|")
  retX = MouseX - Clng(MyArray(0))
  retY = MouseY - Clng(MyArray(1))
End Sub

