Sub Fix()
	C = 0
	Dim D(3)
	
	A = PopKartX - 10
	B = PopKartY + 320
	FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\+3R.bmp", 1.0, intX, intY
	FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\100.bmp", 1.0, intXX, intYY
	While intX > 0 or intXX > 0
		If (intX > 0) Then 
			D(C) = intY - 5
			B = intY + 10
		Else 
			D(C) = intYY - 5
			B = intYY + 10
		End If
		
		FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\+3R.bmp", 1.0, intX, intY
		FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\100.bmp", 1.0, intXX, intYY
		C = C + 1
	Wend
	Y = -1
	A = PopKartX - 10
	B = PopKartY + 320
	FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\+3Y.bmp", 1.0, intX, intY
	While intX > 0
		D(C) = intY - 5
		B = intY + 10
		FindPic A, B, PopKartX + 360, PopKartY + 435, "Attachment:\+3Y.bmp", 1.0, intX, intY
		If (D(C) < D(0)) Then 
			TP = D(0)
			TP1 = D(1)
			D(0) = D(C)
			D(1) = TP
			D(2) = TP1
			Y = 0
		ElseIf (D(C) > D(0) and D(C) < D(1)) Then
			TP = D(1)
			D(1) = D(C)
			D(2) = TP
			Y = 1
		Else 
			Y = 2
		End If
		C = C + 1
	Wend
	
	For i = 0 To 2
		PFCar(i) = False
		Yes(i) = False
	Next

	For i = 0 To 2
		FindX = PopKartX - 10
		FindXX = PopKartX + 190
		FindY = PopKartY + 335 + i * 15
		FindYY = PopKartY + 360 + i * 15
		For j = 0 To 2
			FindPic FindX, D(i), FindXX, D(i) + 20, "Attachment:\改車" & (j + 1) & ".bmp", 1.0, intX, intY
			If (intX > 0) Then 
				PFCar(i) = True
				Goto Re
			End If
		Next
		Rem Re
	Next
	
	PF = 0
	
	For i = 0 To 2
		If (PFCar(i) = True) Then 
			PF = PF + 1
		End If
	Next
	
	If (PF = 3 or (Y>=0 and PF = 2)) Then 
		Goto OK
	End If
	
	If (Safe <= 0) Then 
		For i = 0 To 2
			If (PFCar(i) = True) Then 
				Call Plugin.Bkgnd.LeftClick(PopKart, 507, 598)
				Delay 500
				Call Plugin.Bkgnd.KeyPress(PopKart, 13)
				Delay 500
				If (i <> 0) Then 
					Call Plugin.Bkgnd.LeftClick(PopKart, 680, 398)
					Delay 500
					Select Case (i)
						Case 1
							MoveTo PopKartX + 355, PopKartY + 305
							Delay 500
							LeftClick 1
						Case 2
							MoveTo PopKartX + 355, PopKartY + 320
							Delay 500
							LeftClick 1
					End Select
					Delay 500
				End If
				Call Plugin.Bkgnd.KeyPress(PopKart, 13)
				Safe = 4
				Goto SafeOk
			End If
		Next
	End If
		
	Rem SafeOk
	Delay 500
	/*Call Plugin.Bkgnd.LeftClick(PopKart, 597, 598)
	FindPic 0, 0, 1024, 768, "Attachment:\New.bmp", 0.9, intX, intY
	While intX > 0
		Call Plugin.Bkgnd.KeyPress(PopKart, 13)
		FindPic 0, 0, 1024, 768, "Attachment:\New.bmp", 0.9, intX, intY
	Wend*/
End Sub